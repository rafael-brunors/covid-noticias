CREATE TABLE public.noticia (
    id integer NOT NULL,
    autor character varying(255) NOT NULL,
    noticia text NOT NULL,
    titulo character varying(255) NOT NULL,
    data_atualizacao timestamp without time zone,
    data_criacao timestamp without time zone NOT NULL
);

ALTER TABLE ONLY public.noticia
    ADD CONSTRAINT noticia_pkey PRIMARY KEY (id);
    
ALTER TABLE public.noticia OWNER TO postgres;

ALTER TABLE public.noticia ALTER COLUMN id ADD GENERATED BY DEFAULT AS IDENTITY (
    SEQUENCE NAME public.noticia_id_seq
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1
);